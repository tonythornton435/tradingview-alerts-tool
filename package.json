{
  "name": "add-tradingview-alerts-tool",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "version": "1.0.0",
  "description": "Add TradingView Alerts in bulk",
  "scripts": {
    "build": "tsc",
    "fetch:binance:usdt": "tsc && cross-env QUOTE_ASSET=USDT node lib/fetch-binance-pairs.js > binance_usdt_pairs.csv",
    "fetch:binance:btc": "tsc && env QUOTE_ASSET=BTC node lib/fetch-binance-pairs.js > binance_btc_pairs.csv",
    "add-alerts": "tsc && node lib/add-tradingview-alerts.js"
  },
  "keywords": [
    "tradingview",
    "alerts",
    "3commas",
    "indicator"
  ],
  "author": "",
  "license": "",
  "devDependencies": {
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "binance-api-node": "^0.10.4",
    "csv-parser": "^2.3.3",
    "puppeteer": "^5.2.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "source-map-support": "^0.5.19",
    "yaml": "^1.10.0"
  },
  "private": false
}

{
  "name": "@alleyway/add-tradingview-alerts-tool",
  "repository": {
    "type": "git",
    "url": "git@github.com:alleyway/add-tradingview-alerts-tool.git"
  },
  "bugs": {
    "url": "https://github.com/alleyway/add-tradingview-alerts-tool/issues"
  },
  "description": "Add TradingView Alerts in bulk",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "atat": "./dist/cli.js"
  },
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "!**/*.test.**"
  ],
  "scripts": {
    "build": "npx tsc",
    "atat": "node --experimental-specifier-resolution=node --no-warnings ./dist/cli.js",
    "prepublishOnly": "npm run build",
    "release": "echo 'MAKE SURE YOU RUN ./deploy_master.sh first!' && npx release-it",
    "release-beta": "npx release-it --preRelease=beta",
    "prepare": "husky install",
    "test": "jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "tradingview",
    "alerts",
    "3commas",
    "indicator"
  ],
  "author": "Michael Lake",
  "license": "MIT",
  "version": "1.9.5-beta.2",
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "axios": "^0.25.0",
    "commander": "^8.3.0",
    "consola": "^2.15.3",
    "csv-parser": "^3.0.0",
    "fast-csv": "^4.3.6",
    "kleur": "^4.1.4",
    "pacote": "^12.0.2",
    "prompts": "^2.4.2",
    "puppeteer": "^13.1.1",
    "regex-parser": "^2.2.11",
    "source-map-support": "^0.5.21",
    "strip-bom-stream": "^5.0.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@release-it/bumper": "^3.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.21",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "jest-junit": "^13.0.0",
    "release-it": "^14.12.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.4"
  },
  "private": false,
  "engines": {
    "node": ">= 14.15.0"
  }
}

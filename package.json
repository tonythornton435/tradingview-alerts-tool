{
  "name": "@alleyway/add-tradingview-alerts-tool",
  "repository": {
    "type": "git",
    "url": "git@github.com:alleyway/add-tradingview-alerts-tool.git"
  },
  "bugs": {
    "url": "https://github.com/alleyway/add-tradingview-alerts-tool/issues"
  },
  "version": "1.6.1",
  "description": "Add TradingView Alerts in bulk",
  "type": "module",
  "main": "./lib/index",
  "types": "./lib/interfaces.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "npx tsc",
    "postinstall": "npx tsc",
    "fetch:binance:usdt": "echo 'DEPRECATED! Run the following instead(note lack of colon):  npm run fetch:binance usdt'",
    "fetch:binance:btc": "echo 'DEPRECATED! Run the following instead(note lack of colon):  npm run fetch:binance btc'",
    "fetch:binance:bnb": "echo 'DEPRECATED! Run the following instead(note lack of colon):  npm run fetch:binance bnb'",
    "fetch:binance:eth": "echo 'DEPRECATED! Run the following instead(note lack of colon):  npm run fetch:binance eth'",
    "fetch:binance": "npx tsc && node -r source-map-support/register lib/fetch-binance-pairs.js",
    "fetch:ftx": "npx tsc && node -r source-map-support/register lib/fetch-ftx-pairs.js",
    "add-alerts": "npx tsc && node -r source-map-support/register lib/add-tradingview-alerts.js"
  },
  "keywords": [
    "tradingview",
    "alerts",
    "3commas",
    "indicator"
  ],
  "author": "Michael Lake",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "fast-csv": "^4.3.6",
    "node-fetch": "^2.6.1",
    "puppeteer": "^7.1.0",
    "source-map-support": "^0.5.19",
    "yaml": "^1.10.0"
  },
  "private": false,
  "engines": {
    "node": ">= 12.0.0"
  }
}
